<% content_for(:title, "Contacts") %>

<div class="row index-head">
  <h2>List of Filled Prescriptions</h2>
</div>

<div class="row contacts-ul">
  <ul class="fa-ul">
    <% @fillings.each do |filling| %>
      <% pharmacy = Pharmacy.find(params[:pharmacy_id]) %>
      <li><p><%= link_to "#Prescription filled at #{pharmacy.name}", prescription_filling_path(@prescription,filling),title:"View this filling from #{pharmacy.name}" %></p></li>
    <% end %>
  </ul>
</div>